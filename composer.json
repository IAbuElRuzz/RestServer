{
    "name": "jk/restserver",
    "type": "library",
    "description": "A PHP REST server for providing a very light-weight REST API without aweful routing configuration",
    "require": {
        "php": ">= 5.3.13",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-reflection": "*",
        "ext-spl": "*",
        "geshi/geshi": "dev-master@dev"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "phpspec/phpspec": "~2.0"
    },
    "suggest": {
        "ext-simplexml": "*"
    },
    "keyword": [
        "web service",
        "rest",
        "restful",
        "light weight",
        "server",
        "api"
    ],
    "autoload": {
        "psr-0": {
            "JK\\RestServer\\": "src/"
        }
    },
    "homepage": "https://github.com/jk/RestServer",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Kohl",
            "email": "jens.kohl@gmail.de",
            "role": "Lead",
            "homepage": "https://github.com/jk"
        }
    ],
    "support": {
        "issues": "https://github.com/jk/restserver/issues",
        "wiki": "https://github.com/jk/restserver/wiki"
    },
    "config": {
        "bin-dir": "bin"
    }
}