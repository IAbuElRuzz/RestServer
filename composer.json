{
    "name": "jk/restserver",
    "type": "library",
    "description": "A PHP REST server for providing a very light-weight REST API without aweful routing configuration",
    "require": {
        "php": ">= 5.3.13",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-reflection": "*",
        "ext-spl": "*",
        "geshi/geshi": "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "mockery/mockery": "0.9.3",
        "php-vcr/php-vcr": "1.1.7",
        "php-vcr/phpunit-testlistener-vcr": "1.1.5"
    },
    "suggest": {
        "ext-simplexml": "*"
    },
    "keyword": [
        "web service",
        "rest",
        "restful",
        "light weight",
        "server",
        "api"
    ],
    "autoload": {
        "psr-0": {
            "JK\\RestServer\\": "src/"
        }
    },
    "homepage": "https://github.com/jk/RestServer",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Kohl",
            "email": "jens.kohl@gmail.de",
            "role": "Lead",
            "homepage": "https://github.com/jk"
        }
    ],
    "support": {
        "issues": "https://github.com/jk/restserver/issues",
        "wiki": "https://github.com/jk/restserver/wiki"
    },
    "config": {
        "bin-dir": "bin"
    }
}